@model List<Manager>



<a class="btn btn-info" asp-action="AddManager">Add Manager</a>

<span class="text-danger">@TempData["errMsg"]</span>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <th scope="row">@d.Mid</th>
                <td>@d.Mname</td>
                
                <td>
                    <a class="btn btn-sm btn-danger" asp-action="DelManager" asp-route-id="@d.Mid" onclick="return confirm('Are You Sure?')">Delete</a>

                  
                </td>
            </tr>
        }


    </tbody>
</table>